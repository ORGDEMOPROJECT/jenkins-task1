#!/usr/bin/env groovy
properties([[$class: 'GithubProjectProperty', displayName: '', projectUrlStr: 'https://github.com/ORGDEMOPROJECT/'], 
            parameters([booleanParam(defaultValue: true, description: '', name: 'padmini-1'), 
                        choice(choices: ['name', 'place ', 'animal '], description: '', name: 'padmini-2'), 
                        string(defaultValue: 'branch', description: '', name: 'padmini3')])])

pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'ls -l'
                dir ('foo') {
                 writeFile file:'dummy', text:''
                 }
                }
               }
                stage('Test') {
                 steps {
                  sh 'ls -l'
                  }
                 }
                 stage('Deploy') {
                  steps {
                       echo 'Hello Padmini2'
                       }
                      }
                     }
                    }
